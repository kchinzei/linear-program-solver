{
  "name": "linear-program-solver",
  "version": "1.0.0",
  "description": "Wrapper package for simplex solver",
  "main": "build/Release/linear-program-solver.node",
  "scripts": {
    "preinstall": "sh script/preinstall.sh",
    "build": "tsc",
    "fix": "run-p fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext js,ts .",
    "lint:prettier": "prettier -l \"**/*.{json,md,yml}\"",
    "lint:type": "tsc --noEmit",
    "test": "run-s lint test:*",
    "test:unit": "jest",
    "travis": "run-s preinstall build lint:eslint jest:coveralls",
    "jest:coveralls": "jest --coverage && coveralls < coverage/lcov.info"
  },
  "keywords": [
    "linear programming",
    "simplex",
    "N-API"
  ],
  "author": "Kiyo Chinzei",
  "license": "MIT",
  "contributors": [
    {
      "name": "Kiyo Chinzei",
      "email": "kchinzei@gmail.com"
    }
  ],
  "bundleDependencies": false,
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^3.0.0",
    "linear-program-parser": "^1.0.11"
  },
  "deprecated": false,
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/node": "^11.0.0",
    "jest": "^26.0.0",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.6",
    "eslint": "^7.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4"
  },
  "engines": {
    "node": ">= 6.4.0"
  },
  "homepage": "https://github.com/kchinzei/linear-program-solver",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kchinzei/linear-program-solver.git"
  },
  "types": "./declarations/index.d.ts",
  "gypfile": true
}
